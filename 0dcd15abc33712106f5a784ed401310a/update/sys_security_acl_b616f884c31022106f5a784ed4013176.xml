        <script><![CDATA[var creator = new GlideRecord('sys_user');
creator.get(current.created_by);

var roleCheck = new GlideRecord('sys_user_has_role');
roleCheck.addQuery('user', creator.sys_id);
roleCheck.addQuery('role.name', 'gestor');
roleCheck.query();

if (roleCheck.hasNext() && gs.hasRole('monitor')) {
   answer = false;
} else {
   answer = true;
}
]]></script>
